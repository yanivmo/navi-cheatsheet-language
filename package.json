{
  "name": "navi-cheatsheet-language",
  "displayName": "Navi Cheatsheet Language Support",
  "description": "Syntax highlighting for navi cheatsheet files.",
  "publisher": "yanivmo",
  "version": "1.0.1",
  "license": "MIT",
  "icon": "images/navi-icon-512.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/yanivmo/navi-cheatsheet-language.git"
  },
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "navi",
    "cheatsheet",
    "cheat",
    "syntax highlighting"
  ],
  "contributes": {
    "languages": [
      {
        "id": "navi-cheatsheet",
        "aliases": [
          "Navi Cheatsheet",
          "navi-cheatsheet"
        ],
        "extensions": [
          ".cheat"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "navi-cheatsheet",
        "scopeName": "source.navi-cheatsheet",
        "path": "./syntaxes/navi-cheatsheet.tmLanguage.json"
      }
    ]
  }
}
